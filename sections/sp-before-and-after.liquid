{% schema %}
  {
    "name": "🚀SP - Before and After",
    "settings": [
      {
        "type": "header",
        "content": "Colors",
        "info": "Set colors for the section."
      },
      {
        "type": "color_background",
        "id": "background_color",
        "default": "linear-gradient(164deg, #fff 0%, #fff 0%)",
        "label": "Background"
      },
      {
        "type": "color",
        "id": "text_color",
        "default": "#fff",
        "label": "Text Color"
      },
      {
        "type": "header",
        "content": "Before & After Images",
        "info": "Set the images and captions. Use the same size image for the best effect."
      },
      {
        "type": "image_picker",
        "id": "before_image",
        "label": "Before Image"
      },
      {
        "type": "text",
        "id": "before_caption",
        "label": "Before Caption",
        "default": "Before"
      },
      {
        "type": "image_picker",
        "id": "after_image",
        "label": "After Image"
      },
      {
        "type": "text",
        "id": "after_caption",
        "label": "After Caption",
        "default": "After"
      },
      {
        "type": "header",
        "content": "Dimensions",
        "info": "Set the section dimensions in pixels."
      },
      {
        "type": "number",
        "id": "max_width",
        "default": 1000,
        "label": "Max Width of Section"
      },
      {
        "type": "number",
        "id": "grid_gap",
        "default": 20,
        "label": "Gap Between Reviews"
      },
      {
        "type": "number",
        "id": "padding_top",
        "default": 25,
        "label": "Padding Top"
      },
      {
        "type": "number",
        "id": "padding_bottom",
        "default": 25,
        "label": "Padding Bottom"
      },
      {
        "type": "number",
        "id": "padding_left",
        "default": 10,
        "label": "Padding Left"
      },
      {
        "type": "number",
        "id": "padding_right",
        "default": 10,
        "label": "Padding Right"
      }
    ],
	"blocks": [
    ],
	"presets": [
      {
        "name": "🚀SP - Before and After",
        "category": "Custom"
      }
    ]
  }
{% endschema %}