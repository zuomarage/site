<style>
  #sp-ticon-{{ section.id }} {
    background-image: {{ section.settings.background_color }};
    width: 100%;
  }

  #sp-ticon-{{ section.id }} .sp-ticon {
    position: relative;
    margin: 0 auto;
    max-width: {{ section.settings.max_width }}px;
    padding: {{ section.settings.padding_top }}px {{ section.settings.padding_right }}px {{ section.settings.padding_bottom }}px {{ section.settings.padding_left }}px;
  }

  #sp-ticon-{{ section.id }} .sp-ticon .sp-ticon-grid {
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr; 
    gap: {{ section.settings.grid_gap}}px;
    position: relative;
    z-index: 2;
  }


  @media only screen and (max-width: 800px) {
    #sp-ticon-{{ section.id }} .sp-ticon .sp-ticon-grid {
      display: grid;
      grid-template-columns: {{ section.settings.mobile_columns }};
    }
  }

  #sp-ticon-{{ section.id }} .sp-ticon-col {
    text-align: center;
  }

  #sp-ticon-{{ section.id }} .sp-ticon-col svg {
    position: relative;
    width: 100%;
    max-width: 150px;
    transition: all 0.5s ease;
  }

  #sp-ticon-{{ section.id }} .sp-ticon-col:hover svg {
    transform: scale(1.05);
  }
</style>

