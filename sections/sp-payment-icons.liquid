<style>
  #sp-picon-{{ section.id }} {
    background-image: {{ section.settings.background_color }};
    width: 100%;
  }

  #sp-picon-{{ section.id }} .sp-picon {
    position: relative;
    margin: 0 auto;
    max-width: {{ section.settings.max_width }}px;
    padding: {{ section.settings.padding_top }}px {{ section.settings.padding_right }}px {{ section.settings.padding_bottom }}px {{ section.settings.padding_left }}px;
  }

  #sp-picon-{{ section.id }} .sp-picon .sp-picon-grid {
    display: grid;
    grid-template-columns: 1fr; 
    position: relative;
    z-index: 2;
  }

  @media only screen and (max-width: 800px) {
    #sp-picon-{{ section.id }} .sp-picon .sp-picon-grid {
      display: grid;
      grid-template-columns: {{ section.settings.mobile_columns }};
    }
  }

  #sp-picon-{{ section.id }} .sp-picon-col {
    text-align: center;
  }

  #sp-picon-{{ section.id }} .sp-picon-col img {
    display: inline-block;
    height: {{ section.settings.icon_height }}px;
    margin: 0 {{ section.settings.icon_gap }}px 0 0;
    transition: all 0.5s ease;
  }

  #sp-picon-{{ section.id }} .sp-picon-col img:last-child {
    margin: 0;
  }

  #sp-picon-{{ section.id }} .sp-picon-col img:hover {
    transform: scale(1.05);
  }
</style>

